pipeline {
    agent any
    tools {nodejs "node"}
    stages {
        stage ('sourcecode') {
            steps {
                git branch: 'main', url: 'https://github.com/jyothisalagondla/vsts-task.git'
            }
        }
        stage('install dependencies') {
            steps {
                sh 'npm install'
            }
        }
    }
}